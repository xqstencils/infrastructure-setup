upstream portainer_service {
    server portainer.xqcao.com:9000;
}

server {
    listen 80;
    server_name portainer.xqcao.com;

    location / {
        proxy_pass        http://portainer_service;
        proxy_redirect    off;
        proxy_set_header  HOST $host;
    }
}

server {
    listen 443;
    server_name portainer.xqcao.com;

    location / {
        proxy_pass        http://portainer_service;
        proxy_redirect    off;
        proxy_set_header  HOST $host;
    }
}
